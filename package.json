{
  "name": "03-api-nodejs-solid",
  "version": "1.0.0",
  "description": "GymPass style app.",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node build/server.js",
    "build": "tsup src --out-dir build",
    "lint": "eslint src --ext .ts --fix",
    "test": "vitest run",
    "test:watch": "vitest --project UnitTests",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui --project UnitTests",
    "test:e2e": "vitest run --project E2ETests",
    "test:unit": "vitest run --project UnitTests",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma --loglevel=error",
    "test:install-prisma-environment": "npm link vitest-environment-prisma --loglevel=error",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "2.4.6",
    "@types/node": "22.13.13",
    "@vitest/coverage-v8": "3.0.9",
    "@vitest/ui": "3.0.9",
    "eslint": "9.23.0",
    "neostandard": "0.12.1",
    "npm-run-all": "4.1.5",
    "prisma": "6.5.0",
    "supertest": "7.1.0",
    "tsup": "8.4.0",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.9"
  },
  "dependencies": {
    "@fastify/jwt": "9.1.0",
    "@prisma/client": "6.5.0",
    "bcryptjs": "3.0.2",
    "dayjs": "1.11.13",
    "dotenv": "16.4.7",
    "fastify": "5.2.2",
    "latest": "0.2.0",
    "zod": "3.24.2"
  }
}